<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="500" prefWidth="750" styleClass="vbox-main" stylesheets="/css/ManagerStyle.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.projetenchere.Manager.View.graphicalUserInterface.ManagerGraphicalUserInterface">
    <Label styleClass="title-label" text="Log Box" />
    <ScrollPane fx:id="scrollPane" fitToWidth="true" prefHeight="315.0" prefWidth="750.0" styleClass="scroll-pane">
        <VBox fx:id="messagesVBox" styleClass="vbox-content">
            <padding>
                <Insets bottom="10" left="10" right="10" top="10" />
            </padding>
        </VBox>
    </ScrollPane>
    <Label styleClass="title-label" text="Enchère(s) en cours" />
    <TableView fx:id="auctionsTableView" prefHeight="264.0" prefWidth="750.0" styleClass="table-view">
        <columns>
            <TableColumn fx:id="nameColumn" text="Nom" />
            <TableColumn fx:id="descriptionColumn" text="Description" />
            <TableColumn fx:id="startDateColumn" text="Début" />
            <TableColumn fx:id="endDateColumn" text="Fin" />
            <TableColumn fx:id="statusColumn" text="Status" />
        </columns>
    </TableView>
</VBox>
